<ribbon:RibbonGroupBox
    x:Class="phirSOFT.Applications.FinancePlanner.Core.Views.BookingsRibbon"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
    xmlns:local="clr-namespace:phirSOFT.Applications.FinancePlanner.Core.Views"
    xmlns:ribbon="urn:fluent-ribbon"
    mc:Ignorable="d" 
    Header="Bookings"
    d:DesignHeight="300" d:DesignWidth="300">
    <ribbon:Button Command="{Binding NewCommand}">New</ribbon:Button>
    <ribbon:Button Command="{Binding EditCommand}">Edit</ribbon:Button>
    <ribbon:Button Command="{Binding DeleteCommand}">Delete</ribbon:Button>
</ribbon:RibbonGroupBox>
